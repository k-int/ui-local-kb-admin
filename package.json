{
  "name": "@folio/local-kb-admin",
  "version": "1.2.0",
  "description": "ERM KB Administration for FOLIO with Stripes",
  "main": "src/index.js",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "lint": "eslint .",
    "test": "stripes test karma"
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.9.2",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.2",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^2.10.1",
    "@folio/stripes-cli": "^1.8.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.5.0",
    "mocha": "^6.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "redux": "^3.7.0",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "@folio/stripes-erm-components": "^1.4.0",
    "compose-function": "^3.0.3",
    "final-form": "^4.18.5",
    "final-form-arrays": "^3.0.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "query-string": "^6.6.0",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.1",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.1.1",
    "redux-form": "^7.0.3"
  },
  "peerDependencies": {
    "@folio/stripes": "^2.10.1",
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "redux": "*"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-local-kb-admin.meta.title",
    "route": "/local-kb-admin",
    "home": "/local-kb-admin",
    "hasSettings": true,
    "queryResource": "query",
    "okapiInterfaces": {
      "erm": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.local-kb-admin.enabled",
        "displayName": "UI: ui-local-kb-admin module is enabled",
        "visible": true
      },
      {
        "permissionName": "ui-local-kb-admin.jobs.view",
        "displayName": "Local KB Admin: View agreement jobs",
        "visible": true,
        "subPermissions": [
          "module.local-kb-admin.enabled",
          "erm.jobs.view"
        ]
      },
      {
        "permissionName": "ui-local-kb-admin.jobs.edit",
        "displayName": "Local KB Admin: Create agreement jobs",
        "visible": true,
        "subPermissions": [
          "ui-local-kb-admin.jobs.view",
          "erm.jobs.edit"
        ]
      },
      {
        "permissionName": "ui-local-kb-admin.jobs.delete",
        "displayName": "Local KB Admin: Delete agreement jobs",
        "visible": true,
        "subPermissions": [
          "ui-local-kb-admin.jobs.view",
          "erm.jobs.item.delete"
        ]
      },
      {
        "permissionName": "ui-local-kb-admin.kbs.view",
        "displayName": "Local KB Admin: View remote KB configuration",
        "visible": true,
        "subPermissions": [
          "module.local-kb-admin.enabled",
          "erm.kbs.view"
        ]
      },
      {
        "permissionName": "ui-local-kb-admin.kbs.edit",
        "displayName": "Local KB Admin: Create remote KB configuration",
        "visible": true,
        "subPermissions": [
          "ui-local-kb-admin.kbs.view",
          "erm.kbs.edit"
        ]
      },
      {
        "permissionName": "ui-local-kb-admin.kbs.delete",
        "displayName": "Local KB Admin: Delete remote KB configuration",
        "visible": true,
        "subPermissions": [
          "ui-local-kb-admin.kbs.view",
          "erm.kbs.item.delete"
        ]
      },
      {
        "permissionName": "settings.local-kb-admin.enabled",
        "displayName": "Settings (Local KB Admin): display list of settings pages",
        "visible": true,
        "subPermissions": [
          "settings.enabled"
        ]
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "ERM: Local Kb Admin app for Stripes",
        "title": "LocalKbAdmin"
      }
    ]
  }
}
